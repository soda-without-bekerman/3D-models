$fn=100;
scale (10, 10, 10)
color ([0, 0, 1])
{
//Основание
difference () {
translate ([0, 0, 0.5]) cylinder (3, 1, 1);
translate ([0, 0, 0.9]) cylinder (2.7, 0.9, 0.9);
}
cylinder (0.5, 0.9, 1);
//Крышка
difference () {
translate ([2, 2, 0]) cylinder (0.5, 1, 0.9);
translate ([2.3, 2, -0.1]) cylinder (1, 0.5, 0.5);
}
}
